            "allow_multiple_items": false,
            "allow_version_control": false,
            "extensions": [
                ".ma",
                ".mb",
                ".nk",
                ".hrox",
                ".hip",
                ".hiplc",
                ".hipnc",
                ".blend",
                ".scn",
                ".tvpp",
